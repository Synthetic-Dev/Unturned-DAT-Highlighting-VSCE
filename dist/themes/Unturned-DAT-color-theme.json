{
    "$schema": "vscode://schemas/color-theme",
    "name": "colors",
    "tokenColors":[
        {
            "name": "Property definitions",
            "scope": "keyword.definition",
            "settings": {
                "foreground": "#1c9bd6"
            }
        },
        {
            "name": "Comments",
            "scope": "comment.line.double-slash",
            "settings": {
                "foreground": "#57ad54"
            }
        }
    ]
}